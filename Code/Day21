    <h2>Progress Example</h2>
    <label>Course Progress:</label><br>
    <progress value="40" max="100"></progress>

    <h2>Meter Example</h2>
    <label>Skill Level:</label><br>
    <meter value="6" min="0" max="10"></meter>

